{
  "name": "react-project",
  "version": "1.0.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite --port=3000",
    "start:server": "node ./json-server/index.js",
    "start:dev": "concurrently \"npm start\" \"npm run start:server\"",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:server\"",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:css": "stylelint \"**/*.s[ac]ss\"",
    "lint:css:fix": "stylelint \"**/*.s[ac]ss\" --fix",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npm run storybook:build && npm run test:ui:ci && npm run test:ui:report",
    "test:ui:build": "npm run storybook:build",
    "test:ui:ok": "loki approve",
    "test:ui:upd": "npx loki update --requireReference --reactUri file:./storybook-static",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generate-screenshots-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "chromatic": "chromatic",
    "chromatic:upd": "chromatic --storybook-build-dir ./storybook-static --auto-accept-changes",
    "chromatic:ci": "chromatic --storybook-build-dir ./storybook-static --exit-zero-on-changes --ci",
    "prettier": "npx prettier --check \"**/*.{ts,tsx}\"",
    "prettier:fix": "npx prettier --write \"**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "postinstall": "rm -rf /node_modules/.cache"
  },
